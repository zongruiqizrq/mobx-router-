{
  "presets": [
    "env",
    "react", 
    "stage-0"
  ],
  "plugins": [
    "react-loadable/babel",
    "transform-decorators-legacy",
    "syntax-dynamic-import",
    ["import-inspector", {
      "serverSideRequirePath": true
    }],
    ["transform-runtime",{
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}